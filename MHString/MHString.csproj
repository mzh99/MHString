    <Company>OC Software Solutions</Company>
    <Description>String Utilities and Extensions</Description>
    <PackageTags>OCSS Utility String</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright>© 2022, OC Software Solutions</Copyright>
    <PackageLicenseUrl></PackageLicenseUrl>
